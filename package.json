{
  "name": "boilerplate-backend-node",
  "version": "2.0.1",
  "description": "Boilerplate for a Back in NodeJS",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src",
    "windows-dev": "nodemon -w src --exec \"babel-node src/index.js\"",
    "windows-clean": "rmdir /s /q \"dist\"",
    "windows-build": "npm run windows-clean && babel src -s -D -d dist",
    "macos-clean": "rm -rf dist",
    "macos-build": "npm run macos-clean && babel src -s -D -d dist",
    "macos-dev": "nodemon -w src --exec \"babel-node src/index.js\"",
    "dev": "export NODE_ENV=development && nodemon -w src --exec \"babel-node src/index.js\"",
    "build": "npm run windows-build",
    "production": "npm run build && node dist/index.js",
    "start": "npm run build && export NODE_ENV=production && node dist/index.js",
    "test": "jest tests/mongooseCheckConnection.test.js --noStackTrace --silent && jest -c jest.config.js --noStackTrace --silent",
    "sonar": "sonar-scanner",
    "preinstall": "command -v link-module-alias && link-module-alias clean || true",
    "postinstall": "link-module-alias"
  },
  "_moduleAliases": {
    "$root": "src",
    "$core": "src/core",
    "$middlewares": "src/middlewares",
    "$models": "src/models",
    "$routes": "src/routes",
    "$tests": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/....."
  },
  "author": "....",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-async-generator-functions": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.26.0",
    "jest-junit": "^7.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "link-module-alias": "1.2.0",
    "mongodb-memory-server": "^5.2.0",
    "nodemon": "^1.18.11",
    "sonarqube-scanner": "^2.5.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@babel/register": "^7.4.0",
    "aws-sdk": "^2.442.0",
    "axios": "^0.19.0",
    "babel-jest": "^24.1.0",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "connect-redis": "^3.4.0",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.4",
    "debug": "^4.1.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-useragent": "^1.0.12",
    "googleapis": "^40.0.0",
    "jest": "^24.5.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.14",
    "module-alias": "^2.2.0",
    "moment-timezone": "^0.5.25",
    "mongodb": "^3.1.13",
    "mongoose": "^5.5.2",
    "morgan": "^1.9.1",
    "mysql": "^2.16.0",
    "node-mailjet": "^3.3.1",
    "onesignal-node": "^2.0.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-facebook-token": "^3.3.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.5.0",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "uniqid": "^5.0.3",
    "validator": "^11.0.0",
    "write-file-p": "^1.0.7"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-reporter.xml",
    "indent": 4,
    "sonar56x": true
  }
}
